import java.io.*;

   class sread
   {
      public static void main(String args[]) throws IOException,FileNotFoundException
      {
         FileInputStream fin=new FileInputStream("rStream.txt");
         int i;
         while((i=fin.read())!=-1)
         {
            System.out.print((char)i);
         }
         fin.close();
      }
    /*  catch(Exception e)
      {
         System.out.println(E);
      }*/
   }

