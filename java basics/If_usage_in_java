class If
{
 public static void main(String args[])
     {
        int x;
        int y;
        x=10;
        y=20;
        if(x<y)
          {
               System.out.println("x is less than y");
               x=x*2;
           }
        if(x==y)
          {
               System.out.println("x is now equal y");
               x=x*2;
           }
        if(x>y)
          {
               System.out.println("x is now greater than y");

           }

     }
}
