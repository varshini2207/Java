class Sarray
{
   public static void main(String[] args)
   {
      char[] name=new char[5];
      name[0]='h';
      name[2]='l';
      System.out.println("without for loop \n"+name[0]);
      System.out.println(name[2]+"\nwith for loop");
      for(int i=0;i<5;i++)
      {
         System.out.println(name[i]);
      }
   }
}
