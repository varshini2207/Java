interface Father
{
   abstract void call();
   abstract void talk();
}
interface Mother
{
   abstract void call();
   abstract void talk();
}
class Son implements Father,Mother
{
    public void call()
    {
       System.out.println("Calling:");
    }
    public void talk()
    {
       System.out.println("Talking:");
    }
}
class i1
{
   public static void main(String args[])
   {
      Son s=new Son();
      s.talk();
      s.call();
   }
}
